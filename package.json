{
    "name": "m-ui",
    "version": "3.0.21",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build frontend/ ",
        "build-export": "next build && next export -o frontend",
        "export": "next export frontend -o out",
        "routes": "&& next-nginx-routes -o tmp/nginx-nextjs-routes.conf",
        "start": "next start",
        "lint": "next lint",
        "olddev": "npm run development",
        "olddevelopment": "mix",
        "oldwatch": "mix watch",
        "oldwatch-poll": "mix watch -- --watch-options-poll=1000",
        "oldhot": "mix watch --hot",
        "oldprod": "npm run production",
        "oldproduction": "mix --production",
        "oldclean": "prettier --write \"frontend/**/*.{ts,tsx,css}\"",
        "oldclean-php": "prettier --write app/Http/Controllers/MUIApi",
        "oldpleaserunme": "eslint --fix --ext .jsx,.ts,.js src/"
    },
    "dependencies": {
        "@canonical/react-components": "^0.37.1",
        "@headlessui/react": "^1.4.2",
        "@pg-ui/components": "^1.0.32",
        "@pg-ui/device": "^1.0.1",
        "@pg-ui/i18n": "^1.0.18",
        "@pg-ui/pgv": "^1.0.119",
        "@pg-ui/theme-stanleybet": "^1.0.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.5.0",
        "@types/node": "^17.0.31",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "axios": "^0.27.2",
        "chalk": "4.1.2",
        "dotenv": "^16.0.2",
        "intro.js": "^6.0.0",
        "intro.js-react": "^0.7.1",
        "laravel-mix": "^6.0.49",
        "next": "^13.0.3",
        "next-global-css": "^1.3.1",
        "next-nginx-routes": "https://github.com/FabrizioArmango/next-nginx-routes",
        "react": "^18.2.0",
        "react-charts": "beta",
        "react-datepicker": "^4.8.0",
        "react-dom": "^18.1.0",
        "react-icons": "^4.3.1",
        "react-joyride": "^2.5.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "react-toastify": "^9.1.1",
        "swr": "^1.3.0",
        "typescript": "^4.7.2",
        "vanilla-framework": "^3.12.1",
        "web-vitals": "^2.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.12.1",
        "@babel/preset-react": "^7.17.12",
        "@next/eslint-plugin-next": "^10.0.4",
        "@tailwindcss/forms": "^0.5.2",
        "autoprefixer": "^10.4.2",
        "babel-eslint": "^10.1.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.27.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-next": "13.0.3",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-webpack-plugin": "^3.1.1",
        "glob-all": "^3.3.0",
        "husky": "^8.0.1",
        "media-query-plugin": "^1.5.0",
        "mini-css-extract-plugin": "^2.6.0",
        "postcss": "^8.4.6",
        "prettier": "^2.8.1",
        "process": "^0.11.10",
        "progress-bar-webpack-plugin": "^2.1.0",
        "purgecss-webpack-plugin": "^4.1.3",
        "sass": "^1.51.0",
        "sass-loader": "^13.2.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "tailwindcss": "^3.0.22",
        "terser-webpack-plugin": "^5.2.4",
        "ts-loader": "^9.3.0",
        "url-loader": "^4.1.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1",
        "webpack-livereload-plugin": "1",
        "webpack-node-externals": "^3.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "frontend/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "git add"
        ],
        "*.{html,css,less,ejs}": [
            "git add"
        ]
    },
    "packageManager": "yarn@3.5.0"
}
